@patch {
  $built_in: true
  patch: "Array"
  method: "englishList"
  impl: function(amp) {
    let connector = "and";
    if(amp != undefined && amp) {
      connector = "&";
    }

    switch(this.length) {
      case 0:
        return "";

      case 1:
        return `${this[0]}`;

      case 2:
        return this.join(` ${connector} `);

      default:
        return this.slice(0, -1).join(", ") + `, ${connector} ` + this[this.length - 1];
    }
  }
}

